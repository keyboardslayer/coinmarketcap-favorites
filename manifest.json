{
  "manifest_version": 2,
  "name": "CoinMarketCap Favorites",
  "short_name": "CoinMarketCap Favorites",
  "description": "CoinMarketCap Favorites",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://coinmarketcap.com/"
      ],
      "js": [
        "cmp-favorites.js"
      ],
      "css": [
        "cmp-favorites.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons":
  {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage"
  ]
}